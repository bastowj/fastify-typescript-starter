{
  "name": "fastify-typescript-starter",
  "version": "1.0.0",
  "description": "fastify starter with eslint and typescript",
  "main": "index.js",
  "scripts": {
    "build": "prisma generate && tsc --build",
    "start": "prisma generate && ts-node src/main.ts",
    "dev": "ts-node-dev src/main.ts",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "jest --no-cache --verbose",
    "clear-jest": "jest --clearCache",
    "migrate": "prisma migrate deploy",
    "prisma-generate": "prisma generate",
    "prisma-format": "prisma format"
  },
  "keywords": [],
  "author": "Julian Bastow",
  "license": "ISC",
  "dependencies": {
    "@fastify/swagger": "^7.4.1",
    "@prisma/client": "^4.1.1",
    "fastify": "^4.3.0",
    "fastify-plugin": "^4.0.0",
    "prisma": "^4.1.1",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
